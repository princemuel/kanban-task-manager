{
  "name": "kanban-task-manager",
  "version": "0.1.0",
  "description": "A task manager built with Next.js React Framework, Tailwind CSS, Framer Motion",
  "private": true,
  "repository": "git@github.com:princemuel/kanban-task-manager.git",
  "author": "princemuel <vansomecsam@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "rm -rvf .next && next dev",
    "build": "next build",
    "start": "next start",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "type-check": "tsc --noEmit",
    "gen:prisma": "pnpm exec prisma generate",
    "format:write": "prettier --write \"{app,lib,components}/**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"{app,lib,components}**/*.{ts,tsx,mdx}\" --cache",
    "link:vercel": "vercel link && vercel env pull",
    "postinstall": "pnpm -s run gen:prisma"
  },
  "engines": {
    "node": "18.x",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm",
    "pnpm": "8.x"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@headlessui/tailwindcss": "^0.2.0",
    "@hookform/resolvers": "^3.3.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.3.1",
    "@radix-ui/react-slot": "^1.0.2",
    "@types/node": "20.8.2",
    "@types/react": "18.2.24",
    "@types/react-dom": "18.2.8",
    "@vercel/analytics": "^1.0.2",
    "autoprefixer": "10.4.16",
    "bcrypt": "^5.1.1",
    "cva": "npm:class-variance-authority@^0.6.0",
    "eslint": "8.50.0",
    "eslint-config-next": "13.5.4",
    "framer-motion": "^10.16.4",
    "http-errors": "^2.0.0",
    "immer": "^10.0.3",
    "next": "13.5.4",
    "next-auth": "^4.23.2",
    "next-themes": "^0.2.1",
    "postcss": "8.4.31",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.47.0",
    "sass": "^1.68.0",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2",
    "zod": "^3.22.2",
    "zustand": "^4.4.2"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@types/bcrypt": "^5.0.0",
    "@types/http-errors": "^2.0.2",
    "prettier": "^3.0.3",
    "prettier-plugin-prisma": "^5.0.0",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "prisma": "^5.3.1",
    "svgo-loader": "^4.0.0",
    "tailwind-merge": "^1.14.0",
    "tailwind-scrollbar": "^3.0.5",
    "url-loader": "^4.1.1",
    "vercel": "^32.3.1"
  }
}
