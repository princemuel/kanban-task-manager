{
  "name": "kanban-task-manager",
  "version": "0.1.0",
  "description": "A task manager built with Next.js React Framework, Tailwind CSS, Framer Motion",
  "private": true,
  "repository": "git@github.com:princemuel/kanban-task-manager.git",
  "author": "princemuel <vansomecsam@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "rm -rvf .next && next dev",
    "build": "next build",
    "start": "next start",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "type-check": "tsc --noEmit",
    "gen:prisma": "pnpm exec prisma generate",
    "format:write": "prettier --write \"{app,lib,components}/**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"{app,lib,components}**/*.{ts,tsx,mdx}\" --cache",
    "link:vercel": "vercel link && vercel env pull",
    "postinstall": "pnpm -s run gen:prisma"
  },
  "engines": {
    "node": "18.x",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm",
    "bun": "please-use-pnpm",
    "pnpm": "8.x"
  },
  "dependencies": {
    "@ebay/nice-modal-react": "^1.2.13",
    "@headlessui/react": "^1.7.17",
    "@headlessui/tailwindcss": "^0.2.0",
    "@hookform/resolvers": "^3.3.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.5.2",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@types/node": "20.8.9",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "@vercel/analytics": "^1.1.1",
    "autoprefixer": "10.4.16",
    "clsx": "^2.0.0",
    "eslint": "8.52.0",
    "eslint-config-next": "14.0.0",
    "immer": "^10.0.3",
    "next": "14.0.0",
    "next-auth": "^4.24.5",
    "next-themes": "^0.2.1",
    "postcss": "8.4.31",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.47.0",
    "sass": "^1.69.5",
    "tailwind-variants": "^0.1.17",
    "tailwindcss": "3.3.5",
    "typescript": "5.2.2",
    "zod": "^3.22.4",
    "zustand": "^4.4.4"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "prettier": "^3.0.3",
    "prettier-plugin-prisma": "^5.0.0",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "prisma": "^5.5.2",
    "svgo-loader": "^4.0.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7",
    "tailwind-scrollbar": "^3.0.5",
    "url-loader": "^4.1.1",
    "vercel": "^32.5.0"
  }
}
